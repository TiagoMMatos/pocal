{
  "name": "pocal",
  "version": "0.0.0",
  "description": "Pocal",
  "main": "dist",
  "bin": "cli.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "lint": "eslint src tests",
    "test": "cross-env NODE_ENV=test mochista --opts tests/_/mocha.opts",
    "watch": "run-p \"build -- {@}\" \"test -- {@}\" -- \"--watch\"",
    "index": "create-index src -ri --banner \"/* eslint-disable */\"",
    "start": "node dist"
  },
  "dependencies": {
    "bunyan": "^1.8.5",
    "client-request": "^1.2.0",
    "enquirer": "^0.4.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bunyan-logger": "^2.0.0-pre1",
    "koa-router": "^7.0.1",
    "portscanner": "^2.1.1",
    "prompt-confirm": "^0.2.1",
    "prompt-password": "^0.1.1",
    "require-up": "0.2.0",
    "resolve-redirect": "^0.0.3",
    "source-map-support": "^0.4.5",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-rewire": "^1.0.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-generator": "^2.0.3",
    "chai-like": "^0.1.10",
    "chokidar": "^1.6.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.1",
    "mochista": "^0.3.0",
    "npm-run-all": "^3.1.1",
    "require-up": "^0.0.4",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "assert-request": "^1.0.6"
  }
}
